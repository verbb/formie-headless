{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "vue/multi-word-component-names": "off",
    "vue/no-v-html": "off"
  }
}
